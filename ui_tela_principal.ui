QLineEdit {
    border: 2px solid #3498db;
    border-radius: 20px;
    outline: none;
    font-size: 26px;
	font-weight: 600;
    padding: 4px;
    width: 100%;
    background: white;
    transition: border-color 0.3s ease-in-out, background-color 0.3s ease-in-out;
}

QLineEdit:hover {
    border: 2px solid #2980b9;
    background-color: #f0f8ff;
}

QLineEdit:focus {
    border: 2px solid #1c6ea4;
    background-color: #e0f0ff;
}

QLineEdit::placeholder {
    color: #aaa;
    font-style: italic;
}

QPushButton {
  background-color: #F0F8FF; /* Fundo transparente */
  color: #3C3744; /* Cor do texto */
  border-radius: 20px; /* Bordas arredondadas */
  padding: 12px 24px; /* Espaçamento interno */
  font-size: 16px; /* Tamanho da fonte */
  cursor: pointer; /* Cursor de mão */
  transition: background-color 0.3s, color 0.3s; /* Transição suave */
}

QPushButton:hover {
  background-color: #005AFF;
  border: 2px solid #F0F8FF;
  color: #F0F8FF; /* Cor do texto ao passar o mouse */
}

QPushButton:pressed {
  background-color: #005AFF; /* Cor de fundo ao passar o mouse */
  border: 2px solid #F0F8FF;
  color: #F0F8FF; /* Cor do texto ao passar o mouse */
}

/* Estilização do QComboBox */
QComboBox {
    border: 2px solid #3498db;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
    outline: none;
    font-size: 26px;
    font-weight: 600;
    padding: 6px 12px; /* Ajuste do espaçamento interno */
    width: 100%;
    background: white;
    transition: border-color 0.3s ease-in-out, background-color 0.3s ease-in-out;
}

/* Centraliza o texto */
QComboBox::editable {
    padding: 6px;
    text-align: center;
}

/* Hover */
QComboBox:hover {
    border: 2px solid #2980b9;
    background-color: #f0f8ff;
}

/* Quando o combobox é clicado */
QComboBox:focus, QComboBox:on {
    border: 2px solid #1c6ea4;
    background-color: #e0f0ff;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}

/* Remove a borda do botão dropdown */
QComboBox::drop-down {
    border: none;
    width: 30px;
}

/* Personaliza a seta do dropdown */
QComboBox::down-arrow {
    image: url(:/icons/down-arrow.png); /* Substitua pelo caminho da sua seta */
    width: 16px;
    height: 16px;
}

/* Estiliza a lista suspensa (popup) */
QComboBox QAbstractItemView {
    background-color: white;
    border: 2px solid #3498db;
    selection-background-color: #1c6ea4;
    selection-color: white;
    border-bottom-left-radius: 20px;
    border-bottom-right-radius: 20px;
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
}

/* Centraliza os itens do menu suspenso */
QComboBox QAbstractItemView::item {
    padding: 8px;
    text-align: center;
}
</string>
