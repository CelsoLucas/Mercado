  <property name="styleSheet">
   <string notr="true">/* Estilo geral para a janela principal */
QMainWindow {
    background-color: #F5F6FA;
}

/* Menu superior */
#menu {
    background-color: #FFFFFF;
    border-bottom: 1px solid #E0E4E8;
    padding: 10px;
}

#img_logo {
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 24px;
    font-weight: bold;
    color: #2D3748;
}

#txt_ola_user {
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 16px;
    color: #4A5568;
}

#btn_sair {
    background-color: transparent;
    border: none;
    padding: 5px;
}

#btn_sair:hover {
    background-color: #EDF2F7;
    border-radius: 5px;
}

/* Menu lateral */
#menu_lateral {
    background-color: #2D3748;
    border-right: 1px solid #E0E4E8;
}

#btn_tela_principal, #btn_pdv, #btn_estoque, #btn_relatorios, #btn_configuracoes {
    background-color: #2D3748;
    color: #E2E8F0;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 16px;
    font-weight: 500;
    padding: 15px;
    text-align: left;
    border: none;
    border-radius: 8px;
    margin: 5px 10px;
}

#btn_tela_principal:hover, #btn_pdv:hover, #btn_estoque:hover, 
#btn_relatorios:hover, #btn_configuracoes:hover {
    background-color: #4A5568;
    color: #FFFFFF;
}

/* Área principal */
#_info_principal {
    background-color: #F5F6FA;
    padding: 20px;
}

/* Botões gerais */
QPushButton {
    background-color: #4C51BF;
    color: #FFFFFF;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    font-weight: 500;
    padding: 10px 20px;
    border: none;
    border-radius: 6px;
}

#stackedWidget_6{
color: rgb(211, 211, 211);
}
QPushButton:hover {
    background-color: #5A67D8;
}

QPushButton:pressed {
    background-color: #434190;
}

/* Inputs */
QLineEdit, QTextEdit {
    background-color: #FFFFFF;
    border: 1px solid #E2E8F0;
    border-radius: 6px;
    padding: 8px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    color: #2D3748;
}

QLineEdit:focus, QTextEdit:focus {
    border: 1px solid #4C51BF;
}

/* ComboBox */
QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #E2E8F0;
    border-radius: 6px;
    padding: 8px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    color: #2D3748;
}

QComboBox:hover {
    border: 1px solid #CBD5E0;
}

QComboBox::drop-down {
    border: none;
    width: 20px;
}

QComboBox QAbstractItemView {
    background-color: #FFFFFF;
    border: 1px solid #E2E8F0;
    selection-background-color: #4C51BF;
    selection-color: #FFFFFF;
}

/* Frames */
QFrame {
    background-color: transparent;
}

#layout_dados_sem_tabela, #lado_esquerdo, #lado_direito {
    background-color: #FFFFFF;
    border-radius: 10px;
    padding: 15px;
}

/* Labels */
QLabel {
    font-family: &quot;Segoe UI&quot;, sans-serif;
    color: #2D3748;
}

#txt_resumo_mov, #txt_mov_gaveta, #txt_recebimento_vendas {
    font-size: 18px;
    font-weight: bold;
}

#txt_data, #txt_abertura_db, #txt_operador_db {
    font-size: 14px;
    color: #4A5568;
}

/* Tabelas */
QTreeWidget {
    background-color: #FFFFFF;
    border: 1px solid #E2E8F0;
    border-radius: 6px;
    padding: 5px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
}

QTreeWidget::item {
    padding: 8px;
}

QTreeWidget::item:hover {
    background-color: #EDF2F7;
}

QTreeWidget::item:selected {
    background-color: #4C51BF;
    color: #FFFFFF;
}

QHeaderView::section {
    background-color: #E2E8F0;
    padding: 8px;
    border: none;
    font-weight: bold;
    color: #2D3748;
}

/* Scroll Area */
QScrollArea {
    background-color: transparent;
    border: none;
}

QScrollBar:vertical {
    background: #E2E8F0;
    width: 10px;
    margin: 0;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background: #A0AEC0;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0;
}

/* Tab Widget */
QTabWidget::pane {
    border: 1px solid #E2E8F0;
    background-color: #FFFFFF;
    border-radius: 6px;
}

QTabBar::tab {
    background-color: #E2E8F0;
    color: #4A5568;
    padding: 10px 20px;
    margin-right: 2px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

QTabBar::tab:selected {
    background-color: #FFFFFF;
    color: #2D3748;
    border-bottom: 2px solid #4C51BF;
}

QTabBar::tab:hover:!selected {
    background-color: #EDF2F7;
}

/* Estilo específico para relatórios */
#layout_relatorios_1, #layout_relatorios_2, #layout_relatorios_3, #layout_relatorios_4 {
    background-color: #FFFFFF;
    border-radius: 10px;
    padding: 15px;
}

/* Botões de ação (ex.: Abrir Caixa, Fechar Caixa) */
#btn_abrir_caixa, #btn_fechar_caixa, #btn_sangria, #btn_suprimento {
    background-color: #38A169;
}

#btn_abrir_caixa:hover, #btn_fechar_caixa:hover, #btn_sangria:hover, #btn_suprimento:hover {
    background-color: #48BB78;
}

/* Botões de cancelar */
#btn_cancelar_sangria, #btn_cancelar_suprimento {
    background-color: #E53E3E;
}

#btn_cancelar_sangria:hover, #btn_cancelar_suprimento:hover {
    background-color: #F56565;
}</string>
